{
  "name": "student-course-management",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && cp package.json ./build/&& cp -R configs ./build/  && cp ecosystem.config.js ./build/",
    "jest:test": " jest --detectOpenHandles",
    "start:test": "jest",
    "start:dev": "cross-env NODE_ENV=development nodemon -r tsconfig-paths/register src/server.ts",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts ",
    "start": "node build/server.js",
    "stop": "pm2 stop all",
    "restart": "pm2 restart ecosystem.config.js",
    "delete": "pm2 delete all",
    "clean": "rm -rf ./build && rm -rf ./src//*.js && rm -rf ./src//*.js.map"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongodb-memory-server": "^9.4.0",
    "mongoose": "^8.4.4",
    "morgan": "^1.10.0",
    "on-headers": "^1.0.2",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.8",
    "@types/nodemon": "^1.19.6",
    "@types/on-headers": "^1.0.3",
    "@types/semver": "^7.5.8",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.5.2"
  }
}
